eturn;\n    }\n    // password minimo de 6 caracteres\n    if (password.length < 6) {\n      mostrarAlerta(\n        \"El password debe ser de al menos 6 caracteres\",\n        \"alerta-error\"\n      );\n      return;\n    }\n    // los 2 passwords son iguales\n    if (password !== confpassword) {\n      mostrarAlerta(\"Los passwords no son iguales\", \"alerta-error\");\n      return;\n    }\n    // pasarlo al action\n    registrarUsuario({ nombre, email, password });\n  };\n  return (\n    <div className=\"form-usuario\">\n      {alerta ? (\n        <div className={`alerta ${alerta.categoria}`}>{alerta.msg}</div>\n      ) : null}\n      <div className=\"contenedor-form sombra-dark\">\n        <h1>Obtener una Cuenta</h1>\n\n        <form onSubmit={onSubmit}>\n          <div className=\"campo-form\">\n            <label htmlFor=\"nombre\">Nombre</label>\n            <input\n              type=\"text\"\n              id=\"nombre\"\n              name=\"nombre\"\n              value={nombre}\n              placeholder=\"Tu nombre\"\n              onChange={onChange}\n            />\n          </div>\n          <div className=\"campo-form\">\n            <label htmlFor=\"email\">Email</label>\n            <input\n              type=\"email\"\n              id=\"email\"\n              name=\"email\"\n              value={email}\n              placeholder=\"Tu email\"\n              onChange={onChange}\n            />\n          </div>\n          <div className=\"campo-form\">\n            <label htmlFor=\"password\">Password</label>\n            <input\n              type=\"password\"\n              id=\"password\"\n              name=\"password\"\n              value={password}\n              placeholder=\"Contrase침a\"\n              onChange={onChange}\n            />\n          </div>\n          <div className=\"campo-form\">\n            <label htmlFor=\"password\">Confirmar Password</label>\n            <input\n              type=\"password\"\n              id=\"confpassword\"\n              name=\"confpassword\"\n              value={confpassword}\n              placeholder=\"Confirmar Contrase침a\"\n              onChange={onChange}\n            />\n          </div>\n          <div className=\"campo-form\">\n            <input\n              type=\"submit\"\n              className=\"btn btn-primario btn-block\"\n              value=\"Registrarme\"\n            />\n          </div>\n        </form>\n        <Link to=\"/\" className=\"enlace-cuenta\">\n          Iniciar Sesi칩n\n        </Link>\n      </div>\n    </div>\n  );\n};\n\nexport default Register;\n","/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/context/proyectos/proyectoState.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/types/index.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/context/autenticacion/authContext.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/App.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/auth/Login.js",["76"],"import React, { useState, useContext, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport AlertaContext from \"../../context/alertas/alertaContext\";\nimport AuthContext from \"../../context/autenticacion/authContext\";\nconst Login = ({ history }) => {\n  // extraer valores context\n  const alertaContext = useContext(AlertaContext);\n  const { alerta, mostrarAlerta } = alertaContext;\n\n  const authContext = useContext(AuthContext);\n  const { mensaje, autenticado, iniciarSesion } = authContext;\n\n  //En caso de que el password o usuario no existan\n  useEffect(() => {\n    if (autenticado) {\n   