    }\n    if (mensaje) {\n      mostrarAlerta(mensaje.msg, mensaje.categoria);\n    }\n  }, [mensaje, autenticado, history]);\n  //state para iniciar sesion\n  const [usuario, guardarUsuario] = useState({\n    email: \"\",\n    password: \"\",\n  });\n  //extraer usuario\n  const { email, password } = usuario;\n  const onChange = (e) => {\n    guardarUsuario({\n      ...usuario,\n      [e.target.name]: e.target.value,\n    });\n  };\n  const onSubmit = (e) => {\n    e.preventDefault();\n\n    // validar que no haya campos vacios\n    if (email.trim() === \"\" || password.trim() === \"\") {\n      mostrarAlerta(\"Todos los campos son obligatorios\", \"alerta-error\");\n      return;\n    }\n    // pasarlo al action\n    iniciarSesion({ email, password });\n  };\n  return (\n    <div className=\"form-usuario\">\n      <Alert alert={alerta} />\n      <div className=\"contenedor-form sombra-dark\">\n        <h1 data-cy=\"titulo\">Iniciar Sesion</h1>\n\n        <form data-cy=\"form-login\" onSubmit={onSubmit}>\n          <div className=\"campo-form\">\n            <label htmlFor=\"email\">Email</label>\n            <input\n              data-cy=\"email-input\"\n              type=\"email\"\n              id=\"email\"\n              name=\"email\"\n              placeholder={email}\n              onChange={onChange}\n            />\n          </div>\n          <div className=\"campo-form\">\n            <label htmlFor=\"password\">Password</label>\n            <input\n              data-cy=\"password-input\"\n              type=\"password\"\n              id=\"password\"\n              name=\"password\"\n              placeholder={password}\n              onChange={onChange}\n            />\n          </div>\n          <div className=\"campo-form\">\n            <input\n              data-cy=\"submit-login\"\n              type=\"submit\"\n              className=\"btn btn-primario btn-block\"\n              value=\"Iniciar SesiÃ³n\"\n            />\n          </div>\n        </form>\n\n        <Link\n          data-cy=\"nueva-cuenta\"\n          to=\"/nueva-cuenta\"\n          className=\"enlace-cuenta\"\n        >\n          Obtener Cuenta\n        </Link>\n      </div>\n    </div>\n  );\n};\n\nexport default Login;\n","/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/context/autenticacion/authState.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/proyectos/Proyectos.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/config/axios.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/context/autenticacion/authReducer.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/tareas/ListadoTareas.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/layout/Barra.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/rutas/RutaPrivada.js",["101"],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/proyectos/ListadoProyectos.js",["102","103"],"import React, { useContext, useEffect } from \"react\";\nimport { CSSTransition, TransitionGroup } from \"react-transition-group\";\nimport Proyecto from \"./Proyecto\";\nimport proyectoContext from \"../../context/proyectos/proyectoContext\";\nimport AlertaContext from \"../../context/alertas/alertaContext\";\nconst ListadoProyectos = () => {\n  const proyectosContext = useContext(proyectoContext);\n  const { mensaje, proyectos, obtenerProyectos } = proyectosContext;\n\n  const alertaContext = useContext(AlertaContext);\n  const { alerta, mostrarAlerta } = alertaContext;\n\n  useEffect(() => {\n    obtenerProyectos();\n  }, []);\n\n  useEffect(() => {\n    //si hay un error\n    if (mensaje) {\n      mostrarAlerta(mensaje.msg, mensaje.categoria);\n    }\n  }, [mensaje]);\n\n  if (proyectos.length === 0) return null;\n  return (\n    <ul className=\"listado-proyectos\">\n      {alerta ? (\n        <div className={`alerta ${alerta.categoria}`}>{alerta.msg}</div>\n      ) : null}\n      <TransitionGroup>\n        {proyectos.map((proyecto) => (\n          <CSSTransition key={proyecto._id} classNames=\"proyecto\" timeout={500}>\n            <Proyecto proyecto={proyecto} />\n          </CSSTransition>\n        ))}\n      </TransitionGroup>\n    </ul>\n  );\n};\n\nexport default ListadoProyectos;\n","/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/proyectos/Proyecto.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/context/proyectos/proyectoReducer.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/context/tareas/tareaReducer.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/tareas/FormTarea.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/tareas/Tarea.js",[],"/media/darcdev/Archivos/Usuario/1-ESTUDIO/DESARROLLO/UDEMY/React - principiante a experto/Proyectos/mern-task/cliente/src/components/layout/Alert.js",[],{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","severity":1,"message":"109","line":23,"column":6,"nodeType":"110","endLine":23,"endColumn":37,"suggest